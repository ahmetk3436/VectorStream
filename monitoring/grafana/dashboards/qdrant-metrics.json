{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Qdrant Bağlantı Durumu",
      "type": "stat",
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "red",
                  "text": "BAĞLANTI YOK"
                },
                "1": {
                  "color": "green",
                  "text": "BAĞLI"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "up{job=\"qdrant\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "title": "gRPC Operasyon Süreleri (p95, ms)",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "description": "gRPC üzerinden yapılan Qdrant operasyonlarının 95. yüzdelik dilimdeki gecikme süreleri.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(qdrant_operations_duration_seconds_bucket{protocol=\"grpc\", operation=~\"$operation\"}[2m])) by (le, operation)) * 1000",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 6,
      "title": "Başarılı Operasyon Oranı (%)",
      "type": "stat",
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 95
              },
              {
                "color": "green",
                "value": 99
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "(sum(rate(qdrant_operations_total{status=\"ok\"}[5m])) / sum(rate(qdrant_operations_total[5m]))) * 100",
          "refId": "A"
        }
      ]
    },
    {
      "id": 7,
      "title": "Ortalama Yanıt Süresi (ms)",
      "type": "stat",
      "datasource": "${DS_PROMETHEUS}",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 100
              }
            ]
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "avg(rate(qdrant_operations_duration_seconds_sum[5m]) / rate(qdrant_operations_duration_seconds_count[5m])) * 1000",
          "refId": "A"
        }
      ]
    },
    {
      "id": 9,
      "title": "Hata Oranı Trendi",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "description": "Başarısız olan Qdrant operasyonlarının saniye başına oranı.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "bar",
            "fillOpacity": 80,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(qdrant_operations_total{status=\"error\"}[5m])) by (operation)",
          "legendFormat": "{{operation}} hataları",
          "refId": "A"
        }
      ]
    },
    {
      "id": 8,
      "title": "Operasyon Türü Dağılımı",
      "type": "piechart",
      "datasource": "${DS_PROMETHEUS}",
      "description": "Belirtilen zaman aralığında gerçekleşen toplam Qdrant operasyonlarının türüne göre dağılımı.",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 16,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "options": {
        "displayLabels": [
          "name",
          "percent"
        ],
        "pieType": "donut",
        "reduceOptions": {
          "calcs": [
            "sum"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "expr": "sum by (operation) (increase(qdrant_operations_total{operation=~\"$operation\"}[$__range]))",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "Vektör Yazma Oranı ve Commit Gecikmesi",
      "type": "timeseries",
      "datasource": "${DS_PROMETHEUS}",
      "description": "Vektörlerin saniye başına yazılma oranı ve bu yazma işlemlerinin diske commit edilme gecikmesi (p95).",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {},
          "mappings": []
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Commit Gecikmesi (p95)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ms"
              },
              {
                "id": "custom.axisPlacement",
                "value": "right"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Yazma Oranı (vec/s)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "reqps"
              },
              {
                "id": "custom.fillOpacity",
                "value": 40
              },
              {
                "id": "custom.drawStyle",
                "value": "bar"
              },
              {
                "id": "custom.lineWidth",
                "value": 0
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 16,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(qdrant_operations_total{operation=\"upsert\"}[5m]))",
          "legendFormat": "Yazma Oranı (vec/s)",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(qdrant_storage_commit_duration_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "Commit Gecikmesi (p95)",
          "refId": "A"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 27,
  "style": "dark",
  "tags": [
    "vectorstream",
    "qdrant",
    "iyilestirilmis"
  ],
  "templating": {
    "list": [
      {
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "definition": "label_values(qdrant_operations_total, operation)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "operation",
        "options": [],
        "query": "label_values(qdrant_operations_total, operation)",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      },
      {
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "definition": "label_values(qdrant_collections_vectors_count, collection)",
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "name": "collection",
        "options": [],
        "query": "label_values(qdrant_collections_vectors_count, collection)",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "Qdrant: Detaylı Metrikler",
  "uid": "qdrant-metrics-v2",
  "version": 1
}