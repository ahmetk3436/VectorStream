{
  "dashboard": {
    "id": null,
    "title": "VectorStream - Qdrant Metrikleri",
    "tags": ["vectorstream", "qdrant", "vector-db"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Qdrant Bağlantı Durumu",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"qdrant\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DISCONNECTED"
                  },
                  "1": {
                    "text": "CONNECTED"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Operasyon Süreleri (ms)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(qdrant_request_duration_seconds_bucket[5m])) * 1000",
            "refId": "A",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(qdrant_request_duration_seconds_bucket[5m])) * 1000",
            "refId": "B",
            "legendFormat": "50th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Operasyon Oranı (ops/sec)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qdrant_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{method}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Arama Sonuç Sayısı",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qdrant_search_results_total[5m])",
            "refId": "A",
            "legendFormat": "Results per second"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Toplam Operasyonlar",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(qdrant_operations_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Başarılı Operasyonlar (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "(sum(rate(qdrant_operations_total{status=\"success\"}[5m])) / sum(rate(qdrant_operations_total[5m]))) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Ortalama Yanıt Süresi (ms)",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(qdrant_operations_duration_seconds_sum[5m]) / rate(qdrant_operations_duration_seconds_count[5m])) * 1000",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 500
                }
              ]
            },
            "unit": "ms"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Operasyon Türü Dağılımı",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (operation) (qdrant_operations_total)",
            "refId": "A",
            "legendFormat": "{{operation}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Hata Oranı Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(qdrant_operations_total{status=\"error\"}[5m])",
            "refId": "A",
            "legendFormat": "{{operation}} errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "operation",
          "type": "query",
          "query": "label_values(qdrant_operations_total, operation)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}